identifier > [IIRScram_Identifier *ident]:
  <<$ident = NULL;>>
  id:IDENTIFIER
  <<
    $ident = dynamic_cast<IIRScram_Identifier*>(mytoken(id)->getIIRPtr());
    string temp_text = $ident->convert_to_string();
    copy_location( id, $ident );
